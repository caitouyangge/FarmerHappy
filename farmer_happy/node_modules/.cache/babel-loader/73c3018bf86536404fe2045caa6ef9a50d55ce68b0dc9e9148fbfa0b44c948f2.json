{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { authService } from '../api/auth';\nimport logger from '../utils/logger';\nexport default {\n  name: 'Home',\n  setup() {\n    const router = useRouter();\n    const userInfo = ref({});\n\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    onMounted(() => {\n      logger.lifecycle('Home', 'mounted');\n      const storedUser = localStorage.getItem('user');\n      if (storedUser) {\n        try {\n          userInfo.value = JSON.parse(storedUser);\n          logger.info('HOME', 'Âä†ËΩΩÁî®Êà∑‰ø°ÊÅØÊàêÂäü', {\n            userType: userInfo.value.userType\n          });\n        } catch (error) {\n          logger.error('HOME', 'Ëß£ÊûêÁî®Êà∑‰ø°ÊÅØÂ§±Ë¥•', {}, error);\n          router.push('/login');\n        }\n      } else {\n        logger.warn('HOME', 'Êú™ÊâæÂà∞Áî®Êà∑‰ø°ÊÅØÔºåË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ');\n        router.push('/login');\n      }\n    });\n\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\n    const userInitial = computed(() => {\n      const name = userInfo.value.nickname || userInfo.value.phone || 'U';\n      return name.charAt(0).toUpperCase();\n    });\n\n    // Áî®Êà∑ËßíËâ≤ÊñáÊú¨\n    const userRoleText = computed(() => {\n      const roleMap = {\n        farmer: 'ÂÜúÊà∑',\n        buyer: '‰π∞ÂÆ∂',\n        expert: 'ÊäÄÊúØ‰∏ìÂÆ∂',\n        bank: 'Èì∂Ë°å'\n      };\n      return roleMap[userInfo.value.userType] || 'Êú™Áü•';\n    });\n\n    // Ê¨¢Ëøé‰ø°ÊÅØ\n    const welcomeMessage = computed(() => {\n      const hour = new Date().getHours();\n      let greeting = '‰Ω†Â•Ω';\n      if (hour > 5 && hour < 12) greeting = 'Êó©‰∏äÂ•Ω';else if (hour > 12 && hour < 18) greeting = '‰∏ãÂçàÂ•Ω';else if (hour > 18 && hour < 21) greeting = 'Êôö‰∏äÂ•Ω';else greeting = 'ÂáåÊô®Â•Ω';\n      return `${greeting}Ôºå${userInfo.value.nickname || 'Áî®Êà∑'}`;\n    });\n\n    // ÂâØÊ†áÈ¢ò‰ø°ÊÅØ\n    const subtitleMessage = computed(() => {\n      if (userInfo.value.userType === 'farmer') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂú®ËøôÈáåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅ';\n      } else if (userInfo.value.userType === 'buyer') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂèëÁé∞‰ºòË¥®ÂÜú‰∫ßÂìÅ';\n      }\n      return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞';\n    });\n\n    // Ê†πÊçÆÁî®Êà∑Á±ªÂûãËé∑ÂèñÂèØÁî®ÁöÑÂäüËÉΩÊ®°Âùó\n    const availableModules = computed(() => {\n      const modules = {\n        farmer: [{\n          id: 'trading',\n          name: 'ÂÜú‰∫ßÂìÅ‰∫§Êòì',\n          description: 'ÂèëÂ∏ÉÂíåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅÔºåÊü•Áúã‰∫§ÊòìËÆ¢Âçï',\n          icon: 'üåæ',\n          route: '/trading'\n        }, {\n          id: 'loan',\n          name: 'Ë¥∑Ê¨æ',\n          description: 'Áî≥ËØ∑ÂÜú‰∏öË¥∑Ê¨æÔºåÊü•ÁúãË¥∑Ê¨æËøõÂ∫¶',\n          icon: 'üí∞',\n          route: '/loan'\n        }, {\n          id: 'learning',\n          name: 'ÂÜú‰∏öÁü•ËØÜÂ≠¶‰π†',\n          description: 'Â≠¶‰π†ÂÖàËøõÁöÑÂÜú‰∏öÊäÄÊúØÂíåÁü•ËØÜ',\n          icon: 'üìö',\n          route: '/learning'\n        }],\n        buyer: [{\n          id: 'trading',\n          name: 'ÂÜú‰∫ßÂìÅ‰∫§Êòì',\n          description: 'ÊµèËßà‰ºòË¥®ÂÜú‰∫ßÂìÅÔºå‰∏ãÂçïË¥≠‰π∞',\n          icon: 'üåæ',\n          route: '/trading'\n        }]\n      };\n      return modules[userInfo.value.userType] || [];\n    });\n\n    // ÁôªÂá∫\n    const handleLogout = () => {\n      logger.userAction('LOGOUT_CLICK', {\n        userType: userInfo.value.userType\n      });\n      authService.logout();\n      router.push('/login');\n    };\n\n    // ÂèëÂ∏É‰∫ßÂìÅ\n    const handlePublishProduct = () => {\n      logger.userAction('PUBLISH_PRODUCT_CLICK');\n      alert('ÂèëÂ∏É‰∫ßÂìÅÂäüËÉΩÂºÄÂèë‰∏≠...');\n    };\n\n    // ‰∫ßÂìÅÊìç‰Ωú\n    const handleProductAction = product => {\n      logger.userAction('PRODUCT_ACTION_CLICK', {\n        productId: product.id,\n        userType: userInfo.value.userType\n      });\n      alert(`${productActionText.value}: ${product.name}`);\n    };\n    return {\n      userInfo,\n      userInitial,\n      userRoleText,\n      welcomeMessage,\n      subtitleMessage,\n      products,\n      productActionText,\n      handleLogout,\n      handlePublishProduct,\n      handleProductAction,\n      getDefaultImage\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useRouter","authService","logger","name","setup","router","userInfo","lifecycle","storedUser","localStorage","getItem","value","JSON","parse","info","userType","error","push","warn","userInitial","nickname","phone","charAt","toUpperCase","userRoleText","roleMap","farmer","buyer","expert","bank","welcomeMessage","hour","Date","getHours","greeting","subtitleMessage","availableModules","modules","id","description","icon","route","handleLogout","userAction","logout","handlePublishProduct","alert","handleProductAction","product","productId","productActionText","products","getDefaultImage"],"sources":["D:\\STUDY\\FarmerHappy\\farmer_happy\\src\\home\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"home-container\">\n    <!-- È°∂ÈÉ®ÂØºËà™Ê†è -->\n    <header class=\"header\">\n      <!-- Â∑¶‰æßÁî®Êà∑‰ø°ÊÅØ -->\n      <div class=\"user-info\">\n        <div class=\"avatar\">{{ userInitial }}</div>\n        <div class=\"user-details\">\n          <div class=\"user-name\">{{ userInfo.nickname || userInfo.phone }}</div>\n          <div class=\"user-role\">{{ userRoleText }}</div>\n        </div>\n      </div>\n\n      <!-- Âè≥‰æßÁôªÂá∫ÊåâÈíÆ -->\n      <button class=\"btn-logout\" @click=\"handleLogout\">\n        <span class=\"logout-icon\">‚éã</span>\n        ÁôªÂá∫\n      </button>\n    </header>\n\n    <!-- ‰∏ªÂÜÖÂÆπÂå∫Âüü -->\n    <main class=\"main-content\">\n      <div class=\"content-wrapper\">\n        <!-- Ê¨¢ËøéÊ†áÈ¢ò -->\n        <div class=\"welcome-section\">\n          <h1 class=\"welcome-title\">{{ welcomeMessage }}</h1>\n          <p class=\"welcome-subtitle\">{{ subtitleMessage }}</p>\n        </div>\n\n        <!-- ÂäüËÉΩÊ®°ÂùóÂå∫Âüü -->\n        <div class=\"modules-section\">\n          <h2 class=\"section-title\">ÂäüËÉΩÊ®°Âùó</h2>\n          <div class=\"modules-grid\">\n            <div \n              v-for=\"module in availableModules\" \n              :key=\"module.id\"\n              class=\"module-card\"\n              @click=\"handleModuleClick(module)\"\n            >\n              <div class=\"module-icon\">{{ module.icon }}</div>\n              <h3 class=\"module-name\">{{ module.name }}</h3>\n              <p class=\"module-desc\">{{ module.description }}</p>\n              <div class=\"module-arrow\">‚Üí</div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { authService } from '../api/auth';\nimport logger from '../utils/logger';\n\nexport default {\n  name: 'Home',\n  setup() {\n    const router = useRouter();\n    const userInfo = ref({});\n\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    onMounted(() => {\n      logger.lifecycle('Home', 'mounted');\n      const storedUser = localStorage.getItem('user');\n      if (storedUser) {\n        try {\n          userInfo.value = JSON.parse(storedUser);\n          logger.info('HOME', 'Âä†ËΩΩÁî®Êà∑‰ø°ÊÅØÊàêÂäü', { userType: userInfo.value.userType });\n        } catch (error) {\n          logger.error('HOME', 'Ëß£ÊûêÁî®Êà∑‰ø°ÊÅØÂ§±Ë¥•', {}, error);\n          router.push('/login');\n        }\n      } else {\n        logger.warn('HOME', 'Êú™ÊâæÂà∞Áî®Êà∑‰ø°ÊÅØÔºåË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ');\n        router.push('/login');\n      }\n    });\n\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\n    const userInitial = computed(() => {\n      const name = userInfo.value.nickname || userInfo.value.phone || 'U';\n      return name.charAt(0).toUpperCase();\n    });\n\n    // Áî®Êà∑ËßíËâ≤ÊñáÊú¨\n    const userRoleText = computed(() => {\n      const roleMap = {\n        farmer: 'ÂÜúÊà∑',\n        buyer: '‰π∞ÂÆ∂',\n        expert: 'ÊäÄÊúØ‰∏ìÂÆ∂',\n        bank: 'Èì∂Ë°å'\n      };\n      return roleMap[userInfo.value.userType] || 'Êú™Áü•';\n    });\n\n    // Ê¨¢Ëøé‰ø°ÊÅØ\n    const welcomeMessage = computed(() => {\n      const hour = new Date().getHours();\n      let greeting = '‰Ω†Â•Ω';\n      if (hour>5 && hour < 12) greeting = 'Êó©‰∏äÂ•Ω';\n      else if (hour>12 && hour < 18) greeting = '‰∏ãÂçàÂ•Ω';\n      else if (hour>18 && hour < 21) greeting = 'Êôö‰∏äÂ•Ω';\n      else greeting = 'ÂáåÊô®Â•Ω';\n      \n      return `${greeting}Ôºå${userInfo.value.nickname || 'Áî®Êà∑'}`;\n    });\n\n    // ÂâØÊ†áÈ¢ò‰ø°ÊÅØ\n    const subtitleMessage = computed(() => {\n      if (userInfo.value.userType === 'farmer') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂú®ËøôÈáåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅ';\n      } else if (userInfo.value.userType === 'buyer') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂèëÁé∞‰ºòË¥®ÂÜú‰∫ßÂìÅ';\n      }\n      return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞';\n    });\n\n    // Ê†πÊçÆÁî®Êà∑Á±ªÂûãËé∑ÂèñÂèØÁî®ÁöÑÂäüËÉΩÊ®°Âùó\n    const availableModules = computed(() => {\n      const modules = {\n        farmer: [\n          {\n            id: 'trading',\n            name: 'ÂÜú‰∫ßÂìÅ‰∫§Êòì',\n            description: 'ÂèëÂ∏ÉÂíåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅÔºåÊü•Áúã‰∫§ÊòìËÆ¢Âçï',\n            icon: 'üåæ',\n            route: '/trading'\n          },\n          {\n            id: 'loan',\n            name: 'Ë¥∑Ê¨æ',\n            description: 'Áî≥ËØ∑ÂÜú‰∏öË¥∑Ê¨æÔºåÊü•ÁúãË¥∑Ê¨æËøõÂ∫¶',\n            icon: 'üí∞',\n            route: '/loan'\n          },\n          {\n            id: 'learning',\n            name: 'ÂÜú‰∏öÁü•ËØÜÂ≠¶‰π†',\n            description: 'Â≠¶‰π†ÂÖàËøõÁöÑÂÜú‰∏öÊäÄÊúØÂíåÁü•ËØÜ',\n            icon: 'üìö',\n            route: '/learning'\n          }\n        ],\n        buyer: [\n          {\n            id: 'trading',\n            name: 'ÂÜú‰∫ßÂìÅ‰∫§Êòì',\n            description: 'ÊµèËßà‰ºòË¥®ÂÜú‰∫ßÂìÅÔºå‰∏ãÂçïË¥≠‰π∞',\n            icon: 'üåæ',\n            route: '/trading'\n          }\n        ]\n      };\n\n      return modules[userInfo.value.userType] || [];\n    });\n\n    // ÁôªÂá∫\n    const handleLogout = () => {\n      logger.userAction('LOGOUT_CLICK', { userType: userInfo.value.userType });\n      authService.logout();\n      router.push('/login');\n    };\n\n    // ÂèëÂ∏É‰∫ßÂìÅ\n    const handlePublishProduct = () => {\n      logger.userAction('PUBLISH_PRODUCT_CLICK');\n      alert('ÂèëÂ∏É‰∫ßÂìÅÂäüËÉΩÂºÄÂèë‰∏≠...');\n    };\n\n    // ‰∫ßÂìÅÊìç‰Ωú\n    const handleProductAction = (product) => {\n      logger.userAction('PRODUCT_ACTION_CLICK', { \n        productId: product.id, \n        userType: userInfo.value.userType \n      });\n      alert(`${productActionText.value}: ${product.name}`);\n    };\n\n    return {\n      userInfo,\n      userInitial,\n      userRoleText,\n      welcomeMessage,\n      subtitleMessage,\n      products,\n      productActionText,\n      handleLogout,\n      handlePublishProduct,\n      handleProductAction,\n      getDefaultImage\n    };\n  }\n};\n</script>\n\n<style scoped>\n@import '../assets/styles/theme.css';\n\n.home-container {\n  min-height: 100vh;\n  background: linear-gradient(135deg, #f5f3ff 0%, #ede9fe 100%);\n}\n\n/* È°∂ÈÉ®ÂØºËà™Ê†è */\n.header {\n  background: var(--white);\n  padding: 1rem 2rem;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  box-shadow: 0 2px 8px rgba(107, 70, 193, 0.1);\n  position: sticky;\n  top: 0;\n  z-index: 100;\n}\n\n.user-info {\n  display: flex;\n  align-items: center;\n  gap: 1rem;\n}\n\n.avatar {\n  width: 48px;\n  height: 48px;\n  border-radius: 50%;\n  background: linear-gradient(135deg, var(--primary), var(--primary-light));\n  color: var(--white);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 1.25rem;\n  font-weight: 600;\n  box-shadow: 0 2px 8px rgba(107, 70, 193, 0.3);\n}\n\n.user-details {\n  display: flex;\n  flex-direction: column;\n}\n\n.user-name {\n  font-size: 1rem;\n  font-weight: 600;\n  color: #1a202c;\n}\n\n.user-role {\n  fo