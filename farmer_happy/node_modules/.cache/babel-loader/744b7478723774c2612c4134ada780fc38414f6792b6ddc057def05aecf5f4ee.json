{"ast":null,"code":"import { computed } from 'vue';\nimport logger from '../utils/logger';\nexport default {\n  name: 'ProductCard',\n  props: {\n    product: {\n      type: Object,\n      required: true\n    },\n    isFarmer: {\n      type: Boolean,\n      default: false\n    },\n    selected: {\n      type: Boolean,\n      default: false\n    }\n  },\n  emits: ['select', 'view', 'edit', 'delete', 'on-shelf', 'off-shelf', 'purchase'],\n  setup(props, {\n    emit\n  }) {\n    // ÂàÜÁ±ªÊñáÊú¨\n    const categoryText = computed(() => {\n      const categoryMap = {\n        vegetables: 'Ëî¨Ëèú',\n        fruits: 'Ê∞¥Êûú',\n        grains: 'Á≤ÆÈ£ü',\n        livestock: 'ËÇâÁ±ª',\n        aquatic: 'Ê∞¥‰∫ß'\n      };\n      return categoryMap[props.product.category] || props.product.category || 'ÂÖ∂‰ªñ';\n    });\n\n    // Ê†ºÂºèÂåñÊó•Êúü\n    const formatDate = dateString => {\n      if (!dateString) return 'Êó†';\n      try {\n        const date = new Date(dateString);\n        return date.toLocaleDateString('zh-CN', {\n          year: 'numeric',\n          month: '2-digit',\n          day: '2-digit'\n        });\n      } catch (error) {\n        return 'Êó†';\n      }\n    };\n\n    // Â§ÑÁêÜÈÄâÊã©\n    const handleSelect = event => {\n      logger.userAction('PRODUCT_SELECT', {\n        productId: props.product.product_id,\n        selected: event.target.checked\n      });\n      emit('select', props.product.product_id, event.target.checked);\n    };\n\n    // Â§ÑÁêÜÊü•Áúã\n    const handleView = () => {\n      logger.userAction('PRODUCT_VIEW', {\n        productId: props.product.product_id\n      });\n      emit('view', props.product.product_id);\n    };\n\n    // Â§ÑÁêÜÁºñËæë\n    const handleEdit = () => {\n      logger.userAction('PRODUCT_EDIT', {\n        productId: props.product.product_id\n      });\n      emit('edit', props.product);\n    };\n\n    // Â§ÑÁêÜÂà†Èô§\n    const handleDelete = () => {\n      logger.userAction('PRODUCT_DELETE', {\n        productId: props.product.product_id\n      });\n      emit('delete', props.product.product_id);\n    };\n\n    // Â§ÑÁêÜ‰∏äÊû∂\n    const handleOnShelf = () => {\n      logger.userAction('PRODUCT_ON_SHELF', {\n        productId: props.product.product_id\n      });\n      emit('on-shelf', props.product.product_id);\n    };\n\n    // Â§ÑÁêÜ‰∏ãÊû∂\n    const handleOffShelf = () => {\n      logger.userAction('PRODUCT_OFF_SHELF', {\n        productId: props.product.product_id\n      });\n      emit('off-shelf', props.product.product_id);\n    };\n\n    // Â§ÑÁêÜË¥≠‰π∞\n    const handlePurchase = () => {\n      logger.userAction('PRODUCT_PURCHASE', {\n        productId: props.product.product_id\n      });\n      emit('purchase', props.product);\n    };\n    return {\n      categoryText,\n      formatDate,\n      handleSelect,\n      handleView,\n      handleEdit,\n      handleDelete,\n      handleOnShelf,\n      handleOffShelf,\n      handlePurchase\n    };\n  }\n};","map":{"version":3,"names":["computed","logger","name","props","product","type","Object","required","isFarmer","Boolean","default","selected","emits","setup","emit","categoryText","categoryMap","vegetables","fruits","grains","livestock","aquatic","category","formatDate","dateString","date","Date","toLocaleDateString","year","month","day","error","handleSelect","event","userAction","productId","product_id","target","checked","handleView","handleEdit","handleDelete","handleOnShelf","handleOffShelf","handlePurchase"],"sources":["D:\\STUDY\\FarmerHappy\\farmer_happy\\src\\components\\ProductCard.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-card\" :class=\"{ 'selected': selected }\">\r\n    <!-- ÈÄâÊã©Ê°Ü -->\r\n    <div class=\"card-checkbox\" v-if=\"isFarmer\">\r\n      <input\r\n        type=\"checkbox\"\r\n        :checked=\"selected\"\r\n        @change=\"handleSelect\"\r\n        class=\"checkbox-input\"\r\n      />\r\n    </div>\r\n\r\n    <!-- ‰∫ßÂìÅÂõæÁâá -->\r\n    <div class=\"product-image\">\r\n      <div class=\"image-placeholder\">\r\n        <span class=\"image-icon\">üåæ</span>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ‰∫ßÂìÅ‰ø°ÊÅØ -->\r\n    <div class=\"product-info\">\r\n      <h3 class=\"product-title\" @click=\"handleView\">{{ product.title }}</h3>\r\n      <p class=\"product-category\">{{ categoryText }}</p>\r\n      \r\n      <div class=\"product-meta\">\r\n        <div class=\"meta-item\">\r\n          <span class=\"meta-label\">‰ª∑Ê†º:</span>\r\n          <span class=\"meta-value price\">¬•{{ product.price || 'Èù¢ËÆÆ' }}</span>\r\n        </div>\r\n        <div class=\"meta-item\">\r\n          <span class=\"meta-label\">Â∫ìÂ≠ò:</span>\r\n          <span class=\"meta-value\">{{ product.stock || 0 }} Êñ§</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Êìç‰ΩúÊåâÈíÆ -->\r\n    <div class=\"product-actions\">\r\n      <button class=\"action-btn view-btn\" @click=\"handleView\">\r\n        <span class=\"btn-icon\">üëÅ</span>\r\n        Êü•Áúã\r\n      </button>\r\n      \r\n      <!-- ÂÜúÊà∑Êìç‰Ωú -->\r\n      <template v-if=\"isFarmer\">\r\n        <button class=\"action-btn edit-btn\" @click=\"handleEdit\">\r\n          <span class=\"btn-icon\">‚úèÔ∏è</span>\r\n          ÁºñËæë\r\n        </button>\r\n        \r\n        <button \r\n          v-if=\"product.status === 'draft' || product.status === 'off_shelf'\"\r\n          class=\"action-btn on-shelf-btn\" \r\n          @click=\"handleOnShelf\"\r\n        >\r\n          <span class=\"btn-icon\">üì§</span>\r\n          ‰∏äÊû∂\r\n        </button>\r\n        \r\n        <button \r\n          v-if=\"product.status === 'on_shelf'\"\r\n          class=\"action-btn off-shelf-btn\" \r\n          @click=\"handleOffShelf\"\r\n        >\r\n          <span class=\"btn-icon\">üì•</span>\r\n          ‰∏ãÊû∂\r\n        </button>\r\n        \r\n        <button class=\"action-btn delete-btn\" @click=\"handleDelete\">\r\n          <span class=\"btn-icon\">üóëÔ∏è</span>\r\n          Âà†Èô§\r\n        </button>\r\n      </template>\r\n      \r\n      <!-- ‰π∞ÂÆ∂Êìç‰Ωú -->\r\n      <template v-else>\r\n        <button \r\n          v-if=\"product.status === 'on_shelf' && product.stock > 0\"\r\n          class=\"action-btn purchase-btn\" \r\n          @click=\"handlePurchase\"\r\n        >\r\n          <span class=\"btn-icon\">üõí</span>\r\n          Ë¥≠‰π∞\r\n        </button>\r\n        \r\n        <button \r\n          v-else-if=\"product.status === 'sold_out'\"\r\n          class=\"action-btn disabled-btn\" \r\n          disabled\r\n        >\r\n          <span class=\"btn-icon\">‚ùå</span>\r\n          ÂîÆÁΩÑ\r\n        </button>\r\n      </template>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue';\r\nimport logger from '../utils/logger';\r\n\r\nexport default {\r\n  name: 'ProductCard',\r\n  props: {\r\n    product: {\r\n      type: Object,\r\n      required: true\r\n    },\r\n    isFarmer: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    selected: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  emits: ['select', 'view', 'edit', 'delete', 'on-shelf', 'off-shelf', 'purchase'],\r\n  setup(props, { emit }) {\r\n\r\n    // ÂàÜÁ±ªÊñáÊú¨\r\n    const categoryText = computed(() => {\r\n      const categoryMap = {\r\n        vegetables: 'Ëî¨Ëèú',\r\n        fruits: 'Ê∞¥Êûú',\r\n        grains: 'Á≤ÆÈ£ü',\r\n        livestock: 'ËÇâÁ±ª',\r\n        aquatic: 'Ê∞¥‰∫ß'\r\n      };\r\n      return categoryMap[props.product.category] || props.product.category || 'ÂÖ∂‰ªñ';\r\n    });\r\n\r\n    // Ê†ºÂºèÂåñÊó•Êúü\r\n    const formatDate = (dateString) => {\r\n      if (!dateString) return 'Êó†';\r\n      try {\r\n        const date = new Date(dateString);\r\n        return date.toLocaleDateString('zh-CN', {\r\n          year: 'numeric',\r\n          month: '2-digit',\r\n          day: '2-digit'\r\n        });\r\n      } catch (error) {\r\n        return 'Êó†';\r\n      }\r\n    };\r\n\r\n    // Â§ÑÁêÜÈÄâÊã©\r\n    const handleSelect = (event) => {\r\n      logger.userAction('PRODUCT_SELECT', { \r\n        productId: props.product.product_id,\r\n        selected: event.target.checked \r\n      });\r\n      emit('select', props.product.product_id, event.target.checked);\r\n    };\r\n\r\n    // Â§ÑÁêÜÊü•Áúã\r\n    const handleView = () => {\r\n      logger.userAction('PRODUCT_VIEW', { productId: props.product.product_id });\r\n      emit('view', props.product.product_id);\r\n    };\r\n\r\n    // Â§ÑÁêÜÁºñËæë\r\n    const handleEdit = () => {\r\n      logger.userAction('PRODUCT_EDIT', { productId: props.product.product_id });\r\n      emit('edit', props.product);\r\n    };\r\n\r\n    // Â§ÑÁêÜÂà†Èô§\r\n    const handleDelete = () => {\r\n      logger.userAction('PRODUCT_DELETE', { productId: props.product.product_id });\r\n      emit('delete', props.product.product_id);\r\n    };\r\n\r\n    // Â§ÑÁêÜ‰∏äÊû∂\r\n    const handleOnShelf = () => {\r\n      logger.userAction('PRODUCT_ON_SHELF', { productId: props.product.product_id });\r\n      emit('on-shelf', props.product.product_id);\r\n    };\r\n\r\n    // Â§ÑÁêÜ‰∏ãÊû∂\r\n    const handleOffShelf = () => {\r\n      logger.userAction('PRODUCT_OFF_SHELF', { productId: props.product.product_id });\r\n      emit('off-shelf', props.product.product_id);\r\n    };\r\n\r\n    // Â§ÑÁêÜË¥≠‰π∞\r\n    const handlePurchase = () => {\r\n      logger.userAction('PRODUCT_PURCHASE', { productId: props.product.product_id });\r\n      emit('purchase', props.product);\r\n    };\r\n\r\n    return {\r\n      categoryText,\r\n      formatDate,\r\n      handleSelect,\r\n      handleView,\r\n      handleEdit,\r\n      handleDelete,\r\n      handleOnShelf,\r\n      handleOffShelf,\r\n      handlePurchase\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import '../assets/styles/theme.css';\r\n\r\n.product-card {\r\n  background: var(--white);\r\n  border-radius: 16px;\r\n  box-shadow: 0 4px 12px rgba(107, 70, 193, 0.08);\r\n  overflow: hidden;\r\n  transition: all 0.3s;\r\n  position: relative;\r\n  border: 2px solid transparent;\r\n}\r\n\r\n.product-card:hover {\r\n  transform: translateY(-4px);\r\n  box-shadow: 0 8px 24px rgba(107, 70, 193, 0.15);\r\n}\r\n\r\n.product-card.selected {\r\n  border-color: var(--primary);\r\n  box-shadow: 0 4px 12px rgba(107, 70, 193, 0.2);\r\n}\r\n\r\n/* ÈÄâÊã©Ê°Ü */\r\n.card-checkbox {\r\n  position: absolute;\r\n  top: 1rem;\r\n  left: 1rem;\r\n  z-index: 10;\r\n}\r\n\r\n.checkbox-input {\r\n  width: 18px;\r\n  height: 18px;\r\n  accent-color: var(--primary);\r\n  cursor: pointer;\r\n}\r\n\r\n/* ‰∫ßÂìÅÂõæÁâá */\r\n.product-image {\r\n  position: relative;\r\n  height: 200px;\r\n  background: linear-gradient(135deg, #f0f9ff 0%, #e0f2fe 100%);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.image-placeholder {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 80px;\r\n  height: 80px;\r\n  background: rgba(107, 70, 193, 0.1);\r\n  border-radius: 50%;\r\n}\r\n\r\n.image-icon {\r\n  font-size: 2.5rem;\r\n}\r\n\r\n.status-badge {\r\n  position: absolute;\r\n  top: 1rem;\r\n  right: 1rem;\r\n  padding: 0.25rem 0.75rem;\r\n  border-radius: 12px;\r\n  font-size: 0.75rem;\r\n  font-weight: 500;\r\n  color: var(--white);\r\n}\r\n\r\n.status-draft {\r\n  background: var(--gray-500);\r\n}\r\n\r\n.status-on-shelf {\r\n  background: var(--success);\r\n}\r\n\r\n.status-off-shelf {\r\n  background: #f59e0b;\r\n}\r\n\r\n.status-sold-out {\r\n  background: var(--error);\r\n}\r\n\r\n/* ‰∫ßÂìÅ‰ø°ÊÅØ */\r\n.product-info {\r\n  padding: 1.5rem;\r\n}\r\n\r\n.product-title {\r\n  font-size: 1.125rem;\r\n  font-weight: 600;\r\n  color: #1a202c;\r\n  margin: 0 0 0.5rem 0;\r\n  cursor: pointer;\r\n  transition: color 0.2s;\r\n}\r\n\r\n.product-title:hover {\r\n  color: var(--primary);\r\n}\r\n\r\n.product-category {\r\n  font-size: 0.875rem;\r\n  color: var(--primary);\r\n  font-weight: 500;\r\n  margin: 0 0 0.75rem 0;\r\n}\r\n\r\n.product-meta {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.meta-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.meta-label {\r\n  font-size: 0.75rem;\r\n  color: var(--gray-400);\r\n}\r\n\r\n.meta-value {\r\n  font-size: 0.875rem;\r\n  color: #1a202c;\r\n  font-weight: 500;\r\n}\r\n\r\n.price {\r\n  color: var(--primary);\r\n  font-weight: 600;\r\n  font-size: 1rem;\r\n}\r\n\r\n/* Êìç‰ΩúÊåâÈíÆ */\r\n.product-actions {\r\n  padding: 1rem 1.5rem;\r\n  background: var(--gray-100);\r\n  display: flex;\r\n  gap: 0.5rem;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.action-btn {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 0.25rem;\r\n  padding: 0.5rem 0.75rem;\r\n  border: none;\r\n  border-radius: 6px;\r\n  font-size: 0.75rem;\r\n  font-weight: 500;\r\n  cursor: pointer;\r\n  transition: all 0.2s;\r\n  flex: 1;\r\n  justify-content: center;\r\n  min-width: 60px;\r\n}\r\n\r\n.btn-icon {\r\n  font-size: 0.875rem;\r\n}\r\n\r\n.view-btn {\r\n  background: var(--primary);\r\n  color: var(--white);\r\n}\r\n\r\n.view-btn:hover {\r\n  background: var(--primary-dark);\r\n}\r\n\r\n.edit-btn {\r\n  background: #3b82f6;\r\n  color: var(--white);\r\n}\r\n\r\n.edit-btn:hover {\r\n  background: #2563eb;\r\n}\r\n\r\n.on-shelf-btn {\r\n  background: var(--success);\r\n  color: var(--white);\r\n}\r\n\r\n.on-shelf-btn:hover {\r\n  background: #38a169;\r\n}\r\n\r\n.off-shelf-btn {\r\n  background: #f59e0b;\r\n  color: var(--white);\r\n}\r\n\r\n.off-shelf-btn:hover {\r\n  background: #d97706;\r\n}\r\n\r\n.delete-btn {\r\n  background: var(--error);\r\n  color: var(--white);\r\n}\r\n\r\n.delete-btn:hover {\r\n  background: #c53030;\r\n}\r\n\r\n.purchase-btn {\r\n  background: var(--primary);\r\n  color: var(--white);\r\n}\r\n\r\n.purchase-btn:hover {\r\n  background: var(--primary-dark);\r\n}\r\n\r\n.disabled-btn {\r\n  background: var(--gray-300);\r\n  color: var(--gray-500);\r\n  cursor: not-allowed;\r\n}\r\n\r\n/* ÂìçÂ∫îÂºèËÆæËÆ° */\r\n@media (max-width: 768px) {\r\n  .product-actions {\r\n    flex-direction: column;\r\n  }\r\n\r\n  .action-btn {\r\n    flex: none;\r\n    width: 100%;\r\n  }\r\n\r\n  .product-info {\r\n    padding: 1rem;\r\n  }\r\n\r\n  .product-actions {\r\n    padding: 1rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAmGA,SAASA,QAAO,QAAS,KAAK;AAC9B,OAAOC,MAAK,MAAO,iBAAiB;AAEpC,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,KAAK,EAAE;IACLC,OAAO,EAAE;MACPC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE;IACZ,CAAC;IACDC,QAAQ,EAAE;MACRH,IAAI,EAAEI,OAAO;MACbC,OAAO,EAAE;IACX,CAAC;IACDC,QAAQ,EAAE;MACRN,IAAI,EAAEI,OAAO;MACbC,OAAO,EAAE;IACX;EACF,CAAC;EACDE,KAAK,EAAE,CAAC,QAAQ,EAAE,MAAM,EAAE,MAAM,EAAE,QAAQ,EAAE,UAAU,EAAE,WAAW,EAAE,UAAU,CAAC;EAChFC,KAAKA,CAACV,KAAK,EAAE;IAAEW;EAAK,CAAC,EAAE;IAErB;IACA,MAAMC,YAAW,GAAIf,QAAQ,CAAC,MAAM;MAClC,MAAMgB,WAAU,GAAI;QAClBC,UAAU,EAAE,IAAI;QAChBC,MAAM,EAAE,IAAI;QACZC,MAAM,EAAE,IAAI;QACZC,SAAS,EAAE,IAAI;QACfC,OAAO,EAAE;MACX,CAAC;MACD,OAAOL,WAAW,CAACb,KAAK,CAACC,OAAO,CAACkB,QAAQ,KAAKnB,KAAK,CAACC,OAAO,CAACkB,QAAO,IAAK,IAAI;IAC9E,CAAC,CAAC;;IAEF;IACA,MAAMC,UAAS,GAAKC,UAAU,IAAK;MACjC,IAAI,CAACA,UAAU,EAAE,OAAO,GAAG;MAC3B,IAAI;QACF,MAAMC,IAAG,GAAI,IAAIC,IAAI,CAACF,UAAU,CAAC;QACjC,OAAOC,IAAI,CAACE,kBAAkB,CAAC,OAAO,EAAE;UACtCC,IAAI,EAAE,SAAS;UACfC,KAAK,EAAE,SAAS;UAChBC,GAAG,EAAE;QACP,CAAC,CAAC;MACJ,EAAE,OAAOC,KAAK,EAAE;QACd,OAAO,GAAG;MACZ;IACF,CAAC;;IAED;IACA,MAAMC,YAAW,GAAKC,KAAK,IAAK;MAC9BhC,MAAM,CAACiC,UAAU,CAAC,gBAAgB,EAAE;QAClCC,SAAS,EAAEhC,KAAK,CAACC,OAAO,CAACgC,UAAU;QACnCzB,QAAQ,EAAEsB,KAAK,CAACI,MAAM,CAACC;MACzB,CAAC,CAAC;MACFxB,IAAI,CAAC,QAAQ,EAAEX,KAAK,CAACC,OAAO,CAACgC,UAAU,EAAEH,KAAK,CAACI,MAAM,CAACC,OAAO,CAAC;IAChE,CAAC;;IAED;IACA,MAAMC,UAAS,GAAIA,CAAA,KAAM;MACvBtC,MAAM,CAACiC,UAAU,CAAC,cAAc,EAAE;QAAEC,SAAS,EAAEhC,KAAK,CAACC,OAAO,CAACgC;MAAW,CAAC,CAAC;MAC1EtB,IAAI,CAAC,MAAM,EAAEX,KAAK,CAACC,OAAO,CAACgC,UAAU,CAAC;IACxC,CAAC;;IAED;IACA,MAAMI,UAAS,GAAIA,CAAA,KAAM;MACvBvC,MAAM,CAACiC,U