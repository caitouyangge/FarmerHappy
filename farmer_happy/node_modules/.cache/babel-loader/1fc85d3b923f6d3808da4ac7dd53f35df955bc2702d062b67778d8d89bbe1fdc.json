{"ast":null,"code":"import { ref, computed, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { authService } from '../api/auth';\nimport logger from '../utils/logger';\nexport default {\n  name: 'Home',\n  setup() {\n    const router = useRouter();\n    const userInfo = ref({});\n    const balance = ref(null);\n    const loadingBalance = ref(false);\n\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    onMounted(async () => {\n      logger.lifecycle('Home', 'mounted');\n      const storedUser = localStorage.getItem('user');\n      if (storedUser) {\n        try {\n          userInfo.value = JSON.parse(storedUser);\n          logger.info('HOME', 'Âä†ËΩΩÁî®Êà∑‰ø°ÊÅØÊàêÂäü', {\n            userType: userInfo.value.userType\n          });\n\n          // ÂÆûÊó∂Ëé∑Âèñ‰ΩôÈ¢ù\n          await loadBalance();\n        } catch (error) {\n          logger.error('HOME', 'Ëß£ÊûêÁî®Êà∑‰ø°ÊÅØÂ§±Ë¥•', {}, error);\n          router.push('/login');\n        }\n      } else {\n        logger.warn('HOME', 'Êú™ÊâæÂà∞Áî®Êà∑‰ø°ÊÅØÔºåË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ');\n        router.push('/login');\n      }\n    });\n\n    // Âä†ËΩΩ‰ΩôÈ¢ù\n    const loadBalance = async () => {\n      const userType = userInfo.value.userType;\n      if (userType === 'farmer' || userType === 'buyer' || userType === 'bank') {\n        loadingBalance.value = true;\n        try {\n          const currentBalance = await authService.getBalance(userInfo.value.phone, userType);\n          balance.value = currentBalance;\n          // ÂêåÊó∂Êõ¥Êñ∞localStorage‰∏≠ÁöÑ‰ΩôÈ¢ù\n          userInfo.value.money = currentBalance;\n          localStorage.setItem('user', JSON.stringify(userInfo.value));\n          logger.info('HOME', 'Ëé∑Âèñ‰ΩôÈ¢ùÊàêÂäü', {\n            balance: currentBalance\n          });\n        } catch (error) {\n          logger.error('HOME', 'Ëé∑Âèñ‰ΩôÈ¢ùÂ§±Ë¥•', {}, error);\n          // Â¶ÇÊûúËé∑ÂèñÂ§±Ë¥•Ôºå‰ΩøÁî®localStorage‰∏≠ÁöÑ‰ΩôÈ¢ù\n          balance.value = userInfo.value.money || 0;\n        } finally {\n          loadingBalance.value = false;\n        }\n      }\n    };\n\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\n    const userInitial = computed(() => {\n      const name = userInfo.value.nickname || userInfo.value.phone || 'U';\n      return name.charAt(0).toUpperCase();\n    });\n\n    // Áî®Êà∑ËßíËâ≤ÊñáÊú¨\n    const userRoleText = computed(() => {\n      const roleMap = {\n        farmer: 'ÂÜúÊà∑',\n        buyer: '‰π∞ÂÆ∂',\n        expert: 'ÊäÄÊúØ‰∏ìÂÆ∂',\n        bank: 'Èì∂Ë°å'\n      };\n      return roleMap[userInfo.value.userType] || 'Êó†';\n    });\n\n    // ÊòØÂê¶ÊòæÁ§∫‰ΩôÈ¢ùÔºà‰ªÖÂØπÂÜúÊà∑„ÄÅ‰π∞ÂÆ∂„ÄÅÈì∂Ë°åÊòæÁ§∫Ôºâ\n    const shouldShowBalance = computed(() => {\n      const userType = userInfo.value.userType;\n      return userType === 'farmer' || userType === 'buyer' || userType === 'bank';\n    });\n\n    // Ê†ºÂºèÂåñ‰ΩôÈ¢ù\n    const formattedBalance = computed(() => {\n      const currentBalance = balance.value !== null ? balance.value : userInfo.value.money;\n      if (currentBalance === null || currentBalance === undefined) {\n        return '0.00';\n      }\n      // Á°Æ‰øùÊòØÊï∞Â≠óÁ±ªÂûã\n      const numBalance = typeof currentBalance === 'number' ? currentBalance : parseFloat(currentBalance);\n      if (isNaN(numBalance)) {\n        return '0.00';\n      }\n      return numBalance.toFixed(2);\n    });\n\n    // Ê¨¢Ëøé‰ø°ÊÅØ\n    const welcomeMessage = computed(() => {\n      const hour = new Date().getHours();\n      let greeting = '‰Ω†Â•Ω';\n      if (hour > 5 && hour < 12) greeting = 'Êó©‰∏äÂ•Ω';else if (hour > 12 && hour < 18) greeting = '‰∏ãÂçàÂ•Ω';else if (hour > 18 && hour < 21) greeting = 'Êôö‰∏äÂ•Ω';else greeting = 'ÂáåÊô®Â•Ω';\n      return `${greeting}Ôºå${userInfo.value.nickname || 'Áî®Êà∑'}`;\n    });\n\n    // ÂâØÊ†áÈ¢ò‰ø°ÊÅØ\n    const subtitleMessage = computed(() => {\n      if (userInfo.value.userType === 'farmer') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂú®ËøôÈáåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅ';\n      } else if (userInfo.value.userType === 'buyer') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂèëÁé∞‰ºòË¥®ÂÜú‰∫ßÂìÅ';\n      } else if (userInfo.value.userType === 'expert') {\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞Ôºå‰∏éÂÜúÊà∑ÂàÜ‰∫´‰∏ì‰∏öÁü•ËØÜ';\n      }\n      return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞';\n    });\n\n    // Ê†πÊçÆÁî®Êà∑Á±ªÂûãËé∑ÂèñÂèØÁî®ÁöÑÂäüËÉΩÊ®°Âùó\n    const availableModules = computed(() => {\n      const modules = {\n        farmer: [{\n          id: 'trading',\n          name: 'ÂÜú‰∫ßÂìÅ‰∫§Êòì',\n          description: 'ÂèëÂ∏ÉÂíåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅÔºåÊü•Áúã‰∫§ÊòìËÆ¢Âçï',\n          icon: 'üåæ',\n          route: '/trading'\n        }, {\n          id: 'orders',\n          name: 'ÊàëÁöÑËÆ¢Âçï',\n          description: 'Êü•ÁúãÂíåÁÆ°ÁêÜÊÇ®ÁöÑËÆ¢Âçï',\n          icon: 'üì¶',\n          route: '/orders'\n        }, {\n          id: 'community',\n          name: '‰∏ìÂÆ∂ÂÜúÊà∑‰∫§ÊµÅÂπ≥Âè∞',\n          description: '‰∏é‰∏ìÂÆ∂ÂíåÂÖ∂‰ªñÂÜúÊà∑‰∫§ÊµÅÔºåÂàÜ‰∫´ÁªèÈ™å‰∏éÊèêÈóÆ',\n          icon: 'üí¨',\n          route: '/community'\n        }, {\n          id: 'loan',\n          name: 'Ë¥∑Ê¨æ',\n          description: 'Áî≥ËØ∑ÂÜú‰∏öË¥∑Ê¨æÔºåÊü•ÁúãË¥∑Ê¨æËøõÂ∫¶',\n          icon: 'üí∞',\n          route: '/loan'\n        }],\n        expert: [{\n          id: 'community',\n          name: '‰∏ìÂÆ∂ÂÜúÊà∑‰∫§ÊµÅÂπ≥Âè∞',\n          description: '‰∏éÂÜúÊà∑‰∫§ÊµÅÔºåÂàÜ‰∫´‰∏ì‰∏öÁü•ËØÜ‰∏éËß£Á≠îÈóÆÈ¢ò',\n          icon: 'üí¨',\n          route: '/community'\n        }],\n        buyer: [{\n          id: 'trading',\n          name: 'ÂÜú‰∫ßÂìÅ‰∫§Êòì',\n          description: 'ÊµèËßà‰ºòË¥®ÂÜú‰∫ßÂìÅÔºå‰∏ãÂçïË¥≠‰π∞',\n          icon: 'üåæ',\n          route: '/trading'\n        }, {\n          id: 'orders',\n          name: 'ÊàëÁöÑËÆ¢Âçï',\n          description: 'Êü•ÁúãÂíåÁÆ°ÁêÜÊÇ®ÁöÑËÆ¢Âçï',\n          icon: 'üì¶',\n          route: '/orders'\n        }]\n      };\n      return modules[userInfo.value.userType] || [];\n    });\n\n    // ÁôªÂá∫\n    const handleLogout = () => {\n      logger.userAction('LOGOUT_CLICK', {\n        userType: userInfo.value.userType\n      });\n      authService.logout();\n      router.push('/login');\n    };\n\n    // ÁÇπÂáªÂäüËÉΩÊ®°Âùó\n    const handleModuleClick = module => {\n      logger.userAction('MODULE_CLICK', {\n        moduleId: module.id,\n        moduleName: module.name,\n        userType: userInfo.value.userType\n      });\n\n      // ÊîØÊåÅË∑ØÁî±ÁöÑÊ®°ÂùóÁõ¥Êé•Ë∑≥ËΩ¨\n      if (module.id === 'trading' || module.id === 'community' || module.id === 'orders') {\n        router.push(module.route);\n      } else {\n        // ÂÖ∂‰ªñÊ®°ÂùóÊöÇÊó∂‰ΩøÁî®ÊèêÁ§∫\n        alert(`Âç≥Â∞ÜËøõÂÖ•Ôºö${module.name}\\nÂäüËÉΩÂºÄÂèë‰∏≠...`);\n      }\n    };\n    return {\n      userInfo,\n      balance,\n      loadingBalance,\n      userInitial,\n      userRoleText,\n      shouldShowBalance,\n      formattedBalance,\n      welcomeMessage,\n      subtitleMessage,\n      availableModules,\n      handleLogout,\n      handleModuleClick,\n      loadBalance\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","useRouter","authService","logger","name","setup","router","userInfo","balance","loadingBalance","lifecycle","storedUser","localStorage","getItem","value","JSON","parse","info","userType","loadBalance","error","push","warn","currentBalance","getBalance","phone","money","setItem","stringify","userInitial","nickname","charAt","toUpperCase","userRoleText","roleMap","farmer","buyer","expert","bank","shouldShowBalance","formattedBalance","undefined","numBalance","parseFloat","isNaN","toFixed","welcomeMessage","hour","Date","getHours","greeting","subtitleMessage","availableModules","modules","id","description","icon","route","handleLogout","userAction","logout","handleModuleClick","module","moduleId","moduleName","alert"],"sources":["D:\\STUDY\\FarmerHappy\\farmer_happy\\src\\home\\Home.vue"],"sourcesContent":["<template>\n  <div class=\"home-container\">\n    <!-- È°∂ÈÉ®ÂØºËà™Ê†è -->\n    <header class=\"header\">\n      <!-- Â∑¶‰æßÁî®Êà∑‰ø°ÊÅØ -->\n      <div class=\"user-info\">\n        <div class=\"avatar\">{{ userInitial }}</div>\n        <div class=\"user-details\">\n          <div class=\"user-name\">{{ userInfo.nickname || 'Áî®Êà∑' }}</div>\n          <div class=\"user-phone\">{{ userInfo.phone }}</div>\n          <div class=\"user-role\">{{ userRoleText }}</div>\n          <div v-if=\"shouldShowBalance\" class=\"user-balance\">\n            <span class=\"balance-label\">‰ΩôÈ¢ùÔºö</span>\n            <span class=\"balance-amount\">¬•{{ formattedBalance }}</span>\n          </div>\n        </div>\n      </div>\n\n      <!-- Âè≥‰æßÁôªÂá∫ÊåâÈíÆ -->\n      <button class=\"btn-logout\" @click=\"handleLogout\">\n        <span class=\"logout-icon\">‚éã</span>\n        ÁôªÂá∫\n      </button>\n    </header>\n\n    <!-- ‰∏ªÂÜÖÂÆπÂå∫Âüü -->\n    <main class=\"main-content\">\n      <div class=\"content-wrapper\">\n        <!-- Ê¨¢ËøéÊ†áÈ¢ò -->\n        <div class=\"welcome-section\">\n          <h1 class=\"welcome-title\">{{ welcomeMessage }}</h1>\n          <p class=\"welcome-subtitle\">{{ subtitleMessage }}</p>\n        </div>\n\n        <!-- ÂäüËÉΩÊ®°ÂùóÂå∫Âüü -->\n        <div class=\"modules-section\">\n          <h2 class=\"section-title\">ÂäüËÉΩÊ®°Âùó</h2>\n          <div class=\"modules-grid\">\n            <div \n              v-for=\"module in availableModules\" \n              :key=\"module.id\"\n              class=\"module-card\"\n              @click=\"handleModuleClick(module)\"\n            >\n              <div class=\"module-icon\">{{ module.icon }}</div>\n              <h3 class=\"module-name\">{{ module.name }}</h3>\n              <p class=\"module-desc\">{{ module.description }}</p>\n              <div class=\"module-arrow\">‚Üí</div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </main>\n  </div>\n</template>\n\n<script>\nimport { ref, computed, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { authService } from '../api/auth';\nimport logger from '../utils/logger';\n\nexport default {\n  name: 'Home',\n  setup() {\n    const router = useRouter();\n    const userInfo = ref({});\n    const balance = ref(null);\n    const loadingBalance = ref(false);\n\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\n    onMounted(async () => {\n      logger.lifecycle('Home', 'mounted');\n      const storedUser = localStorage.getItem('user');\n      if (storedUser) {\n        try {\n          userInfo.value = JSON.parse(storedUser);\n          logger.info('HOME', 'Âä†ËΩΩÁî®Êà∑‰ø°ÊÅØÊàêÂäü', { userType: userInfo.value.userType });\n          \n          // ÂÆûÊó∂Ëé∑Âèñ‰ΩôÈ¢ù\n          await loadBalance();\n        } catch (error) {\n          logger.error('HOME', 'Ëß£ÊûêÁî®Êà∑‰ø°ÊÅØÂ§±Ë¥•', {}, error);\n          router.push('/login');\n        }\n      } else {\n        logger.warn('HOME', 'Êú™ÊâæÂà∞Áî®Êà∑‰ø°ÊÅØÔºåË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ');\n        router.push('/login');\n      }\n    });\n\n    // Âä†ËΩΩ‰ΩôÈ¢ù\n    const loadBalance = async () => {\n      const userType = userInfo.value.userType;\n      if (userType === 'farmer' || userType === 'buyer' || userType === 'bank') {\n        loadingBalance.value = true;\n        try {\n          const currentBalance = await authService.getBalance(userInfo.value.phone, userType);\n          balance.value = currentBalance;\n          // ÂêåÊó∂Êõ¥Êñ∞localStorage‰∏≠ÁöÑ‰ΩôÈ¢ù\n          userInfo.value.money = currentBalance;\n          localStorage.setItem('user', JSON.stringify(userInfo.value));\n          logger.info('HOME', 'Ëé∑Âèñ‰ΩôÈ¢ùÊàêÂäü', { balance: currentBalance });\n        } catch (error) {\n          logger.error('HOME', 'Ëé∑Âèñ‰ΩôÈ¢ùÂ§±Ë¥•', {}, error);\n          // Â¶ÇÊûúËé∑ÂèñÂ§±Ë¥•Ôºå‰ΩøÁî®localStorage‰∏≠ÁöÑ‰ΩôÈ¢ù\n          balance.value = userInfo.value.money || 0;\n        } finally {\n          loadingBalance.value = false;\n        }\n      }\n    };\n\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\n    const userInitial = computed(() => {\n      const name = userInfo.value