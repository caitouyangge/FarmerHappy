ents\\ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"modal-overlay\" @click=\"handleOverlayClick\">\r\n    <div class=\"modal-container\" @click.stop>\r\n      <div class=\"modal-header\">\r\n        <h2 class=\"modal-title\">‰∫ßÂìÅËØ¶ÊÉÖ</h2>\r\n        <button class=\"close-btn\" @click=\"handleClose\">\r\n          <span class=\"close-icon\">√ó</span>\r\n        </button>\r\n      </div>\r\n\r\n      <div class=\"modal-content\">\r\n        <div v-if=\"loading\" class=\"loading-container\">\r\n          <div class=\"loading-spinner\"></div>\r\n          <p>Âä†ËΩΩ‰∏≠...</p>\r\n        </div>\r\n\r\n        <div v-else-if=\"error\" class=\"error-container\">\r\n          <div class=\"error-icon\">‚ùå</div>\r\n          <h3>Âä†ËΩΩÂ§±Ë¥•</h3>\r\n          <p>{{ error }}</p>\r\n          <button class=\"retry-btn\" @click=\"loadProductDetail\">\r\n            ÈáçËØï\r\n          </button>\r\n        </div>\r\n\r\n        <div v-else-if=\"product\" class=\"product-detail\">\r\n          <!-- ‰∫ßÂìÅÂõæÁâáÂíåÂü∫Êú¨‰ø°ÊÅØ -->\r\n          <div class=\"product-header\">\r\n            <div class=\"product-image\">\r\n              <div class=\"image-placeholder\">\r\n                <span class=\"image-icon\">üåæ</span>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"product-basic-info\">\r\n              <h1 class=\"product-title\">{{ product.title }}</h1>\r\n              <p class=\"product-category\">{{ categoryText }}</p>\r\n              \r\n              <div class=\"price-section\">\r\n                <span class=\"price-label\">‰ª∑Ê†º:</span>\r\n                <span class=\"price-value\">¬•{{ product.price || 'Èù¢ËÆÆ' }}</span>\r\n              </div>\r\n\r\n              <div class=\"stock-section\">\r\n                <span class=\"stock-label\">Â∫ìÂ≠ò:</span>\r\n                <span class=\"stock-value\">{{ product.stock || 0 }} Êñ§</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- ËØ¶ÁªÜ‰ø°ÊÅØ -->\r\n          <div class=\"product-details\">\r\n            <div class=\"detail-section\">\r\n              <h3 class=\"detail-title\">‰∫ßÂìÅ‰ø°ÊÅØ</h3>\r\n              <div class=\"detail-grid\">\r\n                <div class=\"detail-item\">\r\n                  <span class=\"detail-label\">ÂèëÂ∏ÉÊó∂Èó¥:</span>\r\n                  <span class=\"detail-value\">{{ formatDate(product.created_at) }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <span class=\"detail-label\">Êõ¥Êñ∞Êó∂Èó¥:</span>\r\n                  <span class=\"detail-value\">{{ formatDate(product.updated_at) }}</span>\r\n                </div>\r\n                <div class=\"detail-item\">\r\n                  <span class=\"detail-label\">ÂïÜÂìÅËØ¶ÁªÜ‰ªãÁªç:</span>\r\n                  <span class=\"detail-value\">{{ product.detailed_description || 'Êú™Â°´ÂÜô' }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n\r\n            <!-- ÂÜúÊà∑‰ø°ÊÅØ -->\r\n            <div class=\"detail-section\" v-if=\"product.farmer_info\">\r\n              <h3 class=\"detail-title\">ÂÜúÊà∑‰ø°ÊÅØ</h3>\r\n              <div class=\"farmer-info\">\r\n                <div class=\"farmer-item\">\r\n                  <span class=\"farmer-label\">ÂÜúÊà∑ÂßìÂêç:</span>\r\n                  <span class=\"farmer-value\">{{ product.farmer_info.nickname || 'Êú™Â°´ÂÜô' }}</span>\r\n                </div>\r\n                <div class=\"farmer-item\">\r\n                  <span class=\"farmer-label\">ËÅîÁ≥ªÁîµËØù:</span>\r\n                  <span class=\"farmer-value\">{{ product.farmer_info.phone || 'Êú™Â°´ÂÜô' }}</span>\r\n                </div>\r\n                <div class=\"farmer-item\">\r\n                  <span class=\"farmer-label\">Ê≥®ÂÜåÊó∂Èó¥:</span>\r\n                  <span class=\"farmer-value\">{{ formatDate(product.farmer_info.created_at) }}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Êìç‰ΩúÊåâÈíÆ -->\r\n          <div class=\"action-section\">\r\n            <button class=\"action-btn back-btn\" @click=\"handleClose\">\r\n              <span class=\"btn-icon\">‚Üê</span>\r\n              ËøîÂõû\r\n            </button>\r\n\r\n            <!-- ÂÜúÊà∑Êìç‰Ωú -->\r\n            <template v-if=\"isFarmer\">\r\n              <button class=\"action-btn edit-btn\" @click=\"handleEdit\">\r\n                <span class=\"btn-icon\">‚úèÔ∏è</span>\r\n                ÁºñËæë\r\n              </button>\r\n              \r\n              <button \r\n                v-if=\"product.status === 'draft' || product.status === 'off_shelf'\"\r\n                class=\"action-btn on-shelf-btn\" \r\n                @click=\"handleOnShelf\"\r\n              >\r\n                <span class=\"btn-icon\">üì§</span>\r\n                ‰∏äÊû∂\r\n              </button>\r\n              \r\n              <button \r\n                v-if=\"product.status === 'on_shelf'\"\r\n                class=\"action-btn off-shelf-btn\" \r\n                @click=\"handleOffShelf\"\r\n              >\r\n                <span class=\"btn-icon\">üì•</span>\r\n                ‰∏ãÊû∂\r\n              </button>\r\n              \r\n              <button class=\"action-btn delete-btn\" @click=\"handleDelete\">\r\n                <span class=\"btn-icon\">üóëÔ∏è</span>\r\n                Âà†Èô§\r\n              </button>\r\n            </template>\r\n            \r\n            <!-- ‰π∞ÂÆ∂Êìç‰Ωú -->\r\n            <template v-else>\r\n              <button \r\n                v-if=\"product.status === 'on_shelf' && product.stock > 0\"\r\n                class=\"action-btn purchase-btn\" \r\n                @click=\"handlePurchase\"\r\n              >\r\n                <span class=\"btn-icon\">üõí</span>\r\n                Á´ãÂç≥Ë¥≠‰π∞\r\n              </button>\r\n              \r\n              <button \r\n               