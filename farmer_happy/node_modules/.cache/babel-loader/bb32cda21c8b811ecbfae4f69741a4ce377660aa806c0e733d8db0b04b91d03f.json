come-section\">\r\n          <h1 class=\"welcome-title\">{{ welcomeMessage }}</h1>\r\n          <p class=\"welcome-subtitle\">{{ subtitleMessage }}</p>\r\n        </div>\r\n\r\n        <!-- ÂÜú‰∫ßÂìÅ‰∫§ÊòìÊ®°Âùó -->\r\n        <div class=\"trading-module\">\r\n          <div class=\"module-header\">\r\n            <h2 class=\"module-title\">\r\n              <span class=\"title-icon\">üåæ</span>\r\n              ÂÜú‰∫ßÂìÅ‰∫§Êòì\r\n            </h2>\r\n            <div class=\"module-actions\">\r\n              <button \r\n                v-if=\"userInfo.userType === 'farmer'\" \r\n                class=\"btn btn-primary\"\r\n                @click=\"handlePublishProduct\"\r\n              >\r\n                + ÂèëÂ∏É‰∫ßÂìÅ\r\n              </button>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- ‰∫ßÂìÅÂàóË°® -->\r\n          <div class=\"product-grid\" v-if=\"products.length > 0\">\r\n            <div \r\n              v-for=\"product in products\" \r\n              :key=\"product.id\"\r\n              class=\"product-card\"\r\n            >\r\n              <div class=\"product-image\">\r\n                <img :src=\"product.image || getDefaultImage()\" :alt=\"product.name\">\r\n              </div>\r\n              <div class=\"product-info\">\r\n                <h3 class=\"product-name\">{{ product.name }}</h3>\r\n                <p class=\"product-desc\">{{ product.description }}</p>\r\n                <div class=\"product-footer\">\r\n                  <div class=\"product-price\">\r\n                    <span class=\"price-label\">‰ª∑Ê†ºÔºö</span>\r\n                    <span class=\"price-value\">¬•{{ product.price }}</span>\r\n                    <span class=\"price-unit\">/{{ product.unit }}</span>\r\n                  </div>\r\n                  <button \r\n                    class=\"btn btn-action\"\r\n                    @click=\"handleProductAction(product)\"\r\n                  >\r\n                    {{ productActionText }}\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Á©∫Áä∂ÊÄÅ -->\r\n          <div v-else class=\"empty-state\">\r\n            <div class=\"empty-icon\">üì¶</div>\r\n            <p class=\"empty-text\">ÊöÇÊó†‰∫ßÂìÅ‰ø°ÊÅØ</p>\r\n            <button \r\n              v-if=\"userInfo.userType === 'farmer'\" \r\n              class=\"btn btn-primary\"\r\n              @click=\"handlePublishProduct\"\r\n            >\r\n              ÂèëÂ∏ÉÁ¨¨‰∏Ä‰∏™‰∫ßÂìÅ\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { authService } from '../api/auth';\r\nimport logger from '../utils/logger';\r\n\r\nexport default {\r\n  name: 'Home',\r\n  setup() {\r\n    const router = useRouter();\r\n    const userInfo = ref({});\r\n    const products = ref([]);\r\n\r\n    // Ëé∑ÂèñÁî®Êà∑‰ø°ÊÅØ\r\n    onMounted(() => {\r\n      logger.lifecycle('Home', 'mounted');\r\n      const storedUser = localStorage.getItem('user');\r\n      if (storedUser) {\r\n        try {\r\n          userInfo.value = JSON.parse(storedUser);\r\n          logger.info('HOME', 'Âä†ËΩΩÁî®Êà∑‰ø°ÊÅØÊàêÂäü', { userType: userInfo.value.userType });\r\n          loadProducts();\r\n        } catch (error) {\r\n          logger.error('HOME', 'Ëß£ÊûêÁî®Êà∑‰ø°ÊÅØÂ§±Ë¥•', {}, error);\r\n          router.push('/login');\r\n        }\r\n      } else {\r\n        logger.warn('HOME', 'Êú™ÊâæÂà∞Áî®Êà∑‰ø°ÊÅØÔºåË∑≥ËΩ¨Âà∞ÁôªÂΩïÈ°µ');\r\n        router.push('/login');\r\n      }\r\n    });\r\n\r\n    // Áî®Êà∑ÂêçÈ¶ñÂ≠óÊØç\r\n    const userInitial = computed(() => {\r\n      const name = userInfo.value.nickname || userInfo.value.phone || 'U';\r\n      return name.charAt(0).toUpperCase();\r\n    });\r\n\r\n    // Áî®Êà∑ËßíËâ≤ÊñáÊú¨\r\n    const userRoleText = computed(() => {\r\n      const roleMap = {\r\n        farmer: 'ÂÜúÊà∑',\r\n        buyer: '‰π∞ÂÆ∂',\r\n        expert: 'ÊäÄÊúØ‰∏ìÂÆ∂',\r\n        bank: 'Èì∂Ë°å'\r\n      };\r\n      return roleMap[userInfo.value.userType] || 'Êú™Áü•';\r\n    });\r\n\r\n    // Ê¨¢Ëøé‰ø°ÊÅØ\r\n    const welcomeMessage = computed(() => {\r\n      const hour = new Date().getHours();\r\n      let greeting = '‰Ω†Â•Ω';\r\n      if (hour>5 && hour < 12) greeting = 'Êó©‰∏äÂ•Ω';\r\n      else if (hour < 18) greeting = '‰∏ãÂçàÂ•Ω';\r\n      else greeting = 'Êôö‰∏äÂ•Ω';\r\n      \r\n      return `${greeting}Ôºå${userInfo.value.nickname || 'Áî®Êà∑'}`;\r\n    });\r\n\r\n    // ÂâØÊ†áÈ¢ò‰ø°ÊÅØ\r\n    const subtitleMessage = computed(() => {\r\n      if (userInfo.value.userType === 'farmer') {\r\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂú®ËøôÈáåÁÆ°ÁêÜÊÇ®ÁöÑÂÜú‰∫ßÂìÅ';\r\n      } else if (userInfo.value.userType === 'buyer') {\r\n        return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞ÔºåÂèëÁé∞‰ºòË¥®ÂÜú‰∫ßÂìÅ';\r\n      }\r\n      return 'Ê¨¢ËøéÊù•Âà∞ÂÜú‰πêÂπ≥Âè∞';\r\n    });\r\n\r\n    // ‰∫ßÂìÅÊìç‰ΩúÊåâÈíÆÊñáÊú¨\r\n    const productActionText = computed(() => {\r\n      return userInfo.value.userType === 'farmer' ? 'ÁºñËæë' : 'Êü•ÁúãËØ¶ÊÉÖ';\r\n    });\r\n\r\n    // Âä†ËΩΩ‰∫ßÂìÅÂàóË°®\r\n    const loadProducts = () => {\r\n      // Ê®°Êãü‰∫ßÂìÅÊï∞ÊçÆ\r\n      logger.info('HOME', 'Âä†ËΩΩ‰∫ßÂìÅÂàóË°®');\r\n      products.value = [\r\n        {\r\n          id: 1,\r\n          name: 'ÊúâÊú∫ËãπÊûú',\r\n          description: 'Êñ∞È≤úÈááÊëòÔºåÁªøËâ≤Êó†ÂÖ¨ÂÆ≥',\r\n          price: '8.00',\r\n          unit: 'Êñ§',\r\n          image: null\r\n        },\r\n        {\r\n          id: 2,\r\n          name: 'ÂúüÈ∏°Ëõã',\r\n          description: 'Êï£ÂÖªÂúüÈ∏°ÔºåËê•ÂÖª‰∏∞ÂØå',\r\n          price: '2.00',\r\n          unit: '‰∏™',\r\n          image: null\r\n        },\r\n        {\r\n          id: 3,\r\n          name: 'ÊúâÊú∫Ëî¨Ëèú',\r\n          description: 'ÂΩìÂ≠£Êó∂‰ª§Ëî¨ËèúÔºåÂÅ•Â∫∑Êñ∞È≤ú',\r\n          price: '5.00',\r\n   